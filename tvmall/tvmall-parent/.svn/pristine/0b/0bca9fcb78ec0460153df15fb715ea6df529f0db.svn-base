.focus_list{
    border: solid 4px #0096ff;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    z-index: 999;
    margin-left: -4px;
    margin-top: -4px;
    transition: 200ms;
    -webkit-transition: 200ms;
}



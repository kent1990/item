@charset "utf-8";

.body{
	background: url(../../../images/detail/detail_bg.png) center #264677 no-repeat;
	width:100%;
	height: 720px;
	font-family:'黑体';
}
input{font-family: "黑体";}

.container{
	width:1280px;
	height:720px;
	position:absolute;
	left:50%;
	margin-left:-640px;
	overflow:hidden;
}
.content_detail{
	padding:45px 80px;
}

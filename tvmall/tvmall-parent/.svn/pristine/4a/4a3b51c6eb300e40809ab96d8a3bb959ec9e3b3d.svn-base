.focus_lists1{
    border:4px #8cffec solid;  
	border-radius:4px;
	-webkit-border-radius:4px;
    z-index:999;
      margin:-2px; 
    transition:200ms;
    -webkit-transition:200ms;
}
.focus_lists2{
	border:4px #ff8400 solid;
	border-radius:4px;
	-webkit-border-radius:4px;
    z-index:999;
     margin:-2px; 
    transition:200ms;
    -webkit-transition:200ms;
}
.focus_lists3{
	border:4px #fff solid;
	border-radius:4px;
	-webkit-border-radius:4px;
    z-index:999;
     margin:-2px; 
    transition:200ms;
    -webkit-transition:200ms;
}

.focus_list{
    border: solid 8px #0096ff;
    border-radius: 0px;
    -webkit-border-radius: 0px;
    z-index: 999;
    margin-left: -8px;
    margin-top: -8px;
    transition: 200ms;
    -webkit-transition: 200ms;
}

/*
遮罩层样式
*/
#msgDiv {
    z-index:10001;     
    border:#336699 1px solid;
    position:absolute;
    left:50%;
   
    font-size:12px;

    display: none;
	background-color: #43546f;
}
#bgDiv {
    display: none;
    position: absolute;
    top:0; left:0; 
    height:100%; 
    width:100%; 
    background:#000; 
    opacity:0.6; 
    display:none; 
    z-index:1;
}


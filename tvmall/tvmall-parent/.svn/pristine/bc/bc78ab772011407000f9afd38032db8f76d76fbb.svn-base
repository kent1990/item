@charset "utf-8";
body{
	background-color:#3a5e96;
}
.container{
	width:1280px;
	height:720px;
	position:absolute;
	left:50%;
	margin-left:-640px;
	overflow:hidden;
}
.content_detail{
	padding:45px 80px;
}
.detail_name{
	float:left;
	margin-top:-20px;
}
.detail_time{
	float:right;
	margin-top:5px;
}
.time_minutes,.time_across,.time_date{
	float:left;
}
.time_across{
	margin-left:15px;
	margin-right:10px;
}
.detail_list{
	margin-top:25px;
}
.list_left{
	float:left;
	width:391px;
	height:491px;
	margin-left:-6px;
}
.list_right{
	float:right;
	margin-right:-6px;
}
.list_right_detail div{
	float:left;
}
.list_operate{
	margin-top:20px;
	margin-left:-10px;
	font-size:23px;
	color:#fff;
	line-height:30px;
}
.operate_left{
	float:left;
	width:940px;
}
.operate_right{
	float:right;
}
.list_operate img,span{
	float:left;	
}
.list_operate span{
	margin-left:15px;
	margin-top:0px;
	overflow:hidden;
	height:27px;
	text-align:left;
}

.focus_list{
    border:10px #ff8400 solid;
    border-radius:18px;
    -webkit-border-radius:18px;
    z-index:999;
    /* margin-left:-6px;
    margin-top:-6px; */
    transition:200ms;
    -webkit-transition:200ms;
    filter: progid:DXImageTransform.Microsoft.Shadow(color='#ff8400',Direction=135, Strength=20);/*for ie6,7,8*/
    -moz-box-shadow:0px 0px 20px #ff8400;/*firefox*/
    -webkit-box-shadow:0px 0px 20px #ff8400;/*webkit*/
    box-shadow:0px 0px 20px #ff8400;/*operaæˆ–ie9*/
}
